    "form-data": "^4.0.0",
    "playwright": "^1.55.0",
    "playwright-core": "^1.55.0"
  },
  "scripts": {
    "test": "npx playwright test --reporter=junit",
    "test:local": "npx playwright test",
    "test:single": "npx playwright test tests/single.spec.ts --reporter=junit",
    "test:api": "npx playwright test tests/api.spec.ts",
    "test:allure": "npx playwright test && allure generate allure-results -o allure-report --clean",
    "allure:open": "allure open allure-report",
    "allure:report": "npm run test:allure && npm run allure:open",
    "upload:xray": "node scripts/upload-xray.js"
